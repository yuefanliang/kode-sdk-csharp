{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:5124",

  "Kode": {
    "WorkDir": "./workspace",
    "StoreDir": "./.kode",
    "DefaultProvider": "anthropic",
    "DefaultModel": "claude-sonnet-4-20250514",
    "DefaultSystemPrompt": "You are a helpful AI assistant with access to tools. Use them effectively to help the user.",
    "Tools": "*",
    "Permissions": {
      "Mode": "auto"
    }
  },

  "Skills": {
    "Paths": "skills",
    "Trusted": "demo"
  },

  "Anthropic": {
    "ApiKey": "",
    "BaseUrl": "https://api.anthropic.com",
    "ModelId": "",
    "EnableBetaFeatures": false
  },

  "OpenAI": {
    "ApiKey": "",
    "BaseUrl": "https://api.openai.com/v1",
    "Organization": "",
    "DefaultModel": ""
  },

  "McpServers": {
    "featbit": {
      "transport": "streamableHttp",
      "url": "https://mcp.featbit.co/mcp"
    }
  },

  "OpenTelemetry": {
    "ServiceName": "Kode.Agent.Boilerplate",
    "ServiceVersion": "1.0.0",
    "Enabled": false,
    "Exporter": "console",
    "OtlpEndpoint": "http://localhost:4317"
  }
}
